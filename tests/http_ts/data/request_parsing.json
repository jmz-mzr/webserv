
{
	"suiteFixture": {
		"host": "localhost",
		"port": 8080,
		"headers": { "Host": "localhost:8080" }
	},
	"testCases": [
		{
			"name": "bad_method_name01",
			"requestLine": "GeT / HTTP/1.1\r\n",
			"expectedCode": 400
		},
		{
			"name": "bad_method_name02",
			"requestLine": "LOREMIPSUM / HTTP/1.1\r\n",
			"expectedCode": 405
		},
		{
			"name": "uri_too_long",
			"requestLine": "GET  HTTP/1.1\r\n\r\n",
			"expectedCode": 414
		},
		{
			"name": "invalid_request_line01",
			"requestLine": "GET / HTTP/ 1.1\r\n",
			"expectedCode": 400
		},
		{
			"name": "invalid_request_line02",
			"requestLine": "GET / HTTP/1.1 error\r\n",
			"expectedCode": 400
		},
		{
			"name": "invalid_request_line03",
			"requestLine": "GET HTTP/1.1\r\n",
			"expectedCode": 400
		},
		{
			"name": "version_not_supported",
			"requestLine": "GET / HTTP/5.2\r\n",
			"expectedCode": 505
		},
		{
			"name": "bad_uri01",
			"requestLine": "GET /../../../ HTTP/1.1\r\n",
			"expectedCode": 400
		},
		{
			"name": "bad_uri02",
			"requestLine": "GET * HTTP/1.1\r\n",
			"expectedCode": 400
		},
		{
			"name": "bad_uri03",
			"requestLine": "GET test HTTP/1.1\r\n",
			"expectedCode": 400
		},
		{
			"name": "empty_request",
			"requestLine": "",
			"expectedCode": 400
		},
		{
			"name": "space_after_header_name",
			"requestLine": "GET / HTTP/1.1\r\n",
			"headers": [ "Host : localhost:8080" ],
			"expectedCode": 400
		},
		{
			"name": "no_host_header",
			"requestLine": "GET / HTTP/1.1\r\n",
			"headers": [],
			"expectedCode": 400
		},
		{
			"name": "bare_cr",
			"requestLine": "GET / HTTP/1.1\r",
			"expectedCode": 400
		},
		{
			"name": "bare_cr2",
			"requestLine": "\rGET / HTTP/1.1\r\n",
			"expectedCode": 400
		},
		{
			"name": "tab_sp",
			"requestLine": "GET /\tHTTP/1.1\r\n",
			"expectedCode": 400
		}
	]
}